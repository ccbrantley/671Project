USE PROJECT671;

CREATE TABLE ADMIN (
user_id INT AUTO_INCREMENT NOT NULL,
username CHARACTER VARYING (256) UNIQUE NOT NULL,
password CHARACTER VARYING (256) UNIQUE NOT NULL,
PRIMARY KEY (user_id)
);

CREATE TABLE CUSTOMER (
user_id INT AUTO_INCREMENT NOT NULL,
username CHARACTER VARYING (256) UNIQUE NOT NULL,
password CHARACTER VARYING (256) UNIQUE NOT NULL,
f_name CHARACTER VARYING (256),
l_name CHARACTER VARYING (256),
street CHARACTER VARYING (256),
city CHARACTER VARYING (256),
zip CHARACTER VARYING (256),
state CHARACTER VARYING (256),
country CHARACTER VARYING (256),
credit_card CHARACTER VARYING (256),
PRIMARY KEY (user_id)
);
